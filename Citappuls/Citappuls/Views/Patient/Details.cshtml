@model Citappuls.Data.Entities.Patient
@{
    ViewData["Title"] = "Details";
}



<div class="container mt-5">
    <div class="row">
        <div class="col-md-12">
            <div classs="panel panel-default">
                <div classs="panel-heading">
                    <partial name="_DetailsPatient" />
                </div>
            </div>
        </div>
        <flash dismissable="true" />
        <div>

            <a asp-action="Confirm" asp-route-id="@Model?.Id" class="btn btn-outline-success">Confirmar</a>
            <a asp-action="Consum" asp-route-id="@Model?.Id" class="btn btn-outline-primary">Consumar</a>
            <a asp-action="Reagend" asp-route-id="@Model?.Id" class="btn btn-outline-warning">Re-Agendar</a>
            <a asp-action="Cancel" asp-route-id="@Model?.Id" class="btn btn-outline-danger">Cancelar</a>
            <a asp-action="Index" class="btn btn-outline-secondary">Regresar</a>
        </div>
    </div>
</div>